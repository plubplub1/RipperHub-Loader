local keys = {
    "MUXuTYWzItlV2xeN",
    "TqKLuEMgp3CFePOZ",
    "7AvkT9n3fjEyeWdZ",
    "oJwbmt7QxPe8DN5K",
    "8C5bkiHaWmrFVSq4",
    "OL9wuAdKIaRsF41W",
    "f8GFAt6jUWasewIh",
    "jowUfYZB3DPzeSNv",
    "UslqhkvjELYHxcRM",
    "xJ3Tpqv7Rzlt9cFD",
    "RrHSjbhZm9e7yLzc",
    "fhqcVeGPRapCEmAW",
    "xU9fpKs027XLYHrO",
    "mbSGeL5PzAIFh792",
    "0yrCEzOot61mfTPn",
    "B34ST-K53Y-70R-S9UP"
}

local counter = 1
local keyCheck
for i,v in pairs(keys) do
    if counter == #keys then
    -- ไม่ whitelist
    keys = ""
    game.Players.LocalPlayer:Kick("please enter your key")
    else
        if v == _G.Key then
            -- whitelist-
            print("Whitelist Succes, Enjoy :)")
            loadstring(game:HttpGet("https://raw.githubusercontent.com/hajibeza/RIPPER-HUB/main/NEWBF.lua"))()
            keyCheck = _G.Key
            keys = ""
        else
            counter = counter +1
        end
    end
end

while true do
    if _G.Key == keyCheck then
        -- ปลอมคีย์ - เปลี่ยนแปลงคีย์
    else
        game.Players.LocalPlayer:Kick("invaild key")
    end
    wait()
end
